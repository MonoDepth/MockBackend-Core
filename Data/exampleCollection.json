{
  "Name": "Example collection",
  "EndPoint": "*",
  "HttpPort": 7000,
  "HttpsPort": 7001,
  "ProxyServerEndpoint": "127.0.0.1",
  "ProxyServerPort": 7652,
  "DomainsToRelay": [
    "sitedelta.com"
  ],
  "Controllers": [
    {
      "Path": "test",
      "Method": "GET",
      "Status": 200,
      "ContentType": "application/text",
      "Body": "GET Example"
    },
    {
      "Path": "test",
      "Method": "POST",
      "Status": 201,
      "Delay": 2000,
      "ContentType": "application/text",
      "Body": "POST Example"
    },
    {
      "Path": "test",
      "Method": "DELETE",
      "Status": 200,
      "ContentType": "application/text",
      "Body": "DELETE Example"
    },
    {
      "Path": "movie/full",
      "Method": "GET",
      "Status": 200,
      "ContentType": "application/mp4",
      "BodyFile": "../../Data/sample_960x540.mp4"
    }
  ]
}